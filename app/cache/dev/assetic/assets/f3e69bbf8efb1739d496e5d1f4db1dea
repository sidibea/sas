$(document).ready(function(){var b="integrity-skin";function a(c){$.cookie(b,c);document.location.reload()}$("#default").click(function(){a("default")});$("#red").click(function(){a("red")});$("#yellow").click(function(){a("yellow")});$("#orange").click(function(){a("orange")})});$(document).ready(function(){$(".switcher").animate({left:"-200px"});$(".switcher h3 a").click(function(a){a.preventDefault();var b=$(".switcher");if(b.css("left")==="-200px"){$(".switcher").animate({left:"20px"},300)}else{$(".switcher").animate({left:"-200px"},300)}})});