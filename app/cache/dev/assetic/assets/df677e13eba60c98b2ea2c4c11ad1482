var twitterFetcher=function(){function l(k){return k.replace(/<b[^>]*>(.*?)<\/b>/gi,function(m,p){return p}).replace(/class=".*?"|data-query-source=".*?"|dir=".*?"|rel=".*?"/gi,"")}function f(p,u){for(var t=[],s=new RegExp("(^| )"+u+"( |$)"),r=p.getElementsByTagName("*"),m=0,q=r.length;m<q;m++){s.test(r[m].className)&&t.push(r[m])}return t}var j="",g=20,i=!0,d=[],D=!1,b=!0,a=!0,C=null,o=!0,h=!0,n=null,e=!0,c=!1;return{fetch:function(k){void 0===k.maxTweets&&(k.maxTweets=20);void 0===k.enableLinks&&(k.enableLinks=!0);void 0===k.showUser&&(k.showUser=!0);void 0===k.showTime&&(k.showTime=!0);void 0===k.dateFunction&&(k.dateFunction="default");void 0===k.showRetweet&&(k.showRetweet=!0);void 0===k.customCallback&&(k.customCallback=null);void 0===k.showInteraction&&(k.showInteraction=!0);void 0===k.showImages&&(k.showImages=!1);if(D){d.push(k)}else{D=!0;j=k.domId;g=k.maxTweets;i=k.enableLinks;a=k.showUser;b=k.showTime;h=k.showRetweet;C=k.dateFunction;n=k.customCallback;e=k.showInteraction;c=k.showImages;var m=document.createElement("script");m.type="text/javascript";m.src="//cdn.syndication.twimg.com/widgets/timelines/"+k.id+"?&lang="+(k.lang||"en")+"&callback=twitterFetcher.callback&suppress_response_codes=true&rnd="+Math.random();document.getElementsByTagName("head")[0].appendChild(m)}},callback:function(x){var v=document.createElement("div");v.innerHTML=x.body;"undefined"===typeof v.getElementsByClassName&&(o=!1);x=[];var s=[],r=[],q=[],w=[],m=[],t=0;if(o){for(v=v.getElementsByClassName("tweet");t<v.length;){0<v[t].getElementsByClassName("retweet-credit").length?w.push(!0):w.push(!1);if(!w[t]||w[t]&&h){x.push(v[t].getElementsByClassName("e-entry-title")[0]),m.push(v[t].getAttribute("data-tweet-id")),s.push(v[t].getElementsByClassName("p-author")[0]),r.push(v[t].getElementsByClassName("dt-updated")[0]),void 0!==v[t].getElementsByClassName("inline-media")[0]?q.push(v[t].getElementsByClassName("inline-media")[0]):q.push(void 0)}t++}}else{for(v=f(v,"tweet");t<v.length;){x.push(f(v[t],"e-entry-title")[0]),m.push(v[t].getAttribute("data-tweet-id")),s.push(f(v[t],"p-author")[0]),r.push(f(v[t],"dt-updated")[0]),void 0!==f(v[t],"inline-media")[0]?q.push(f(v[t],"inline-media")[0]):q.push(void 0),0<f(v[t],"retweet-credit").length?w.push(!0):w.push(!1),t++}}x.length>g&&(x.splice(g,x.length-g),s.splice(g,s.length-g),r.splice(g,r.length-g),w.splice(g,w.length-g),q.splice(g,q.length-g));v=[];t=x.length;for(w=0;w<t;){if("string"!==typeof C){var u=new Date(r[w].getAttribute("datetime").replace(/-/g,"/").replace("T"," ").split("+")[0]),u=C(u);r[w].setAttribute("aria-label",u);if(x[w].innerText){if(o){r[w].innerText=u}else{var p=document.createElement("p"),k=document.createTextNode(u);p.appendChild(k);p.setAttribute("aria-label",u);r[w]=p}}else{r[w].textContent=u}}u="";i?(a&&(u+='<div class="user">'+l(s[w].innerHTML)+"</div>"),u+='<p class="tweet">'+l(x[w].innerHTML)+"</p>",b&&(u+='<p class="timePosted">'+r[w].getAttribute("aria-label")+"</p>")):x[w].innerText?(a&&(u+='<p class="user">'+s[w].innerText+"</p>"),u+='<p class="tweet">'+x[w].innerText+"</p>",b&&(u+='<p class="timePosted">'+r[w].innerText+"</p>")):(a&&(u+='<p class="user">'+s[w].textContent+"</p>"),u+='<p class="tweet">'+x[w].textContent+"</p>",b&&(u+='<p class="timePosted">'+r[w].textContent+"</p>"));e&&(u+='<p class="interact"><a href="https://twitter.com/intent/tweet?in_reply_to='+m[w]+'" class="twitter_reply_icon">Reply</a><a href="https://twitter.com/intent/retweet?tweet_id='+m[w]+'" class="twitter_retweet_icon">Retweet</a><a href="https://twitter.com/intent/favorite?tweet_id='+m[w]+'" class="twitter_fav_icon">Favorite</a></p>');c&&void 0!==q[w]&&(p=q[w],void 0!==p?(p=p.innerHTML.match(/data-srcset="([A-z0-9%_\.-]+)/i)[0],p=decodeURIComponent(p).split('"')[1]):p=void 0,u+='<div class="media"><img src="'+p+'" alt="Image from tweet" /></div>');v.push(u);w++}if(null===n){x=v.length;s=0;r=document.getElementById(j);for(q="<ul>";s<x;){q+="<li>"+v[s]+"</li>",s++}r.innerHTML=q+"</ul>"}else{n(v)}D=!1;0<d.length&&(twitterFetcher.fetch(d[0]),d.splice(0,1))}}}();