(function(b){var a=b(window);var c=a.height();a.resize(function(){c=a.height()});b.fn.parallax=function(k,h,j){function e(){var f=a.scrollTop();n.each(function(){var l=b(this);var i=l.offset().top;var o=g(l);if(i+o<f||i>f+c){return}n.css("backgroundPosition",k+" "+Math.round((i-f)*h)+"px")})}var n=b(this);var g;var d;var m=0;n.each(function(){d=n.offset().top});if(j){g=function(f){return f.outerHeight(true)}}else{g=function(f){return f.height()}}if(arguments.length<1||k===null){k="50%"}if(arguments.length<2||h===null){h=0.1}if(arguments.length<3||j===null){j=true}a.bind("scroll",e).resize(e);e()}})(jQuery);var $window=jQuery(window);var windowHeight=$window.height();$window.resize(function(){windowHeight=$window.height()});$.fn.hparallax=function(d,f,h){var g=$(this);var e;var a;var i=0;if(h){e=function(j){return j.outerHeight(true)}}else{e=function(j){return j.height()}}if(arguments.length<1||d===null){d="50%"}if(arguments.length<2||f===null){f=0.5}if(arguments.length<3||h===null){h=true}f=($(this).data("parallax_sense"))/100;d="0px";var c=$window.scrollTop();function b(){var j=$window.scrollTop();g.each(function(){a=$(this).offset().top;var l=$(this);var q=l.offset().top;var k=e(l);if(q+k<j||q>j+windowHeight){return}var n=$(this).css("backgroundPosition");var m=n.indexOf("px");var p=n.substring(0,m);var o=0;if(c-j<=0){o=parseInt(p)-parseInt(f*(Math.abs(c-j)))}else{o=parseInt(p)+parseInt(f*(c-j));if(o>0){o=0}}$(this).css("backgroundPosition",o+"px "+d)});c=j}$window.bind("scroll",b).resize(b);b()};$.fn.animatedparallax=function(d,c){var h=$(this);var a=h.data("animation-repeat");h.css({"background-repeat":a});var f=10;if(h.attr("data-parallax_sense")!=""){f=h.attr("data-parallax_sense")}f=100-f;var g=h.attr("data-bg-animation-type");var b=h.attr("data-bg-animation");var e=0;var i=g;setInterval(function(j){if(b=="right-animation"||b=="bottom-animation"){e-=1}else{e+=1}h.css("backgroundPosition",(i=="horizontal")?e+"px 0":"0 "+e+"px")},f)};