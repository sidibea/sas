(function(a){if(a(window).width()>1024){a(window).on("scroll load",function(){a(".animate").each(function(){var e=a(this).data("animation");if(e==null){e="fadeIn"}var d=a(this).data("animation-delay");if(d==null){d="0"}var h=a(this).data("animation-speed");if(h==null){h="1000"}var g={"animation-delay":d+"ms","-webkit-animation-delay":d+"ms","animation-duration":h+"ms","-webkit-animation-duration":h+"ms"};var b=a(this).data("animation-offset");if(b==null){b="90%"}var f=a(window).height()*(parseFloat(b)/100)+a(window).scrollTop();var c=a(this).offset().top;if(c<f){a(this).addClass("visible").addClass(e).css(g)}})})}else{a(".animate").addClass("visible")}}(jQuery));